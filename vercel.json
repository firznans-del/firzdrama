{
    "version": 2,
    "builds": [
        {
            "src": "web_server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "web_server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/",
            "dest": "/public/firzdrama.html"
        }
    ]
}